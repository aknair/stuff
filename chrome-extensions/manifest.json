{
  "manifest_version": 3,
  "name": "URL Tinker",
  "description": "Tinker with the URL",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.example.com/*/*"
      ],
      "run_at": "document_start",
      "js": [
        "content-script.js"
      ]
    }
  ]
}
